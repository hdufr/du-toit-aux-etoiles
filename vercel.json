{
    "version": 2,
    "buildCommand": null,
    "installCommand": null,
    "outputDirectory": ".",
    "framework": "html",
    "routes": [
        { "src": "/", "dest": "/index.html" },
        { "src": "/audio/(.*)", "dest": "/audio/$1" },
        { "src": "/images/(.*)", "dest": "/images/$1" },
        { "src": "/styles.css", "dest": "/styles.css" },
        { "src": "/script.js", "dest": "/script.js" },
        { "src": "/playlist.json", "dest": "/playlist.json" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
            ]
        }
    ]
}
